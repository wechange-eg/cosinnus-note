#: .\templates\cosinnus_note\notifications\note_comment_posted.html:1
#, python-format
msgid ""
"Hi, %(receiver_name)s!\n"
"\n"
"%(sender_name)s has posted a comment on a news post you wrote in %(team_name)"
"s:\n"
"\n"
"\"%(comment_text)s\"\n"
"\n"
"\n"
"on your post:\n"
"\n"
"\"%(note_text)s\"\n"
"\n"
"\n"
"Follow this link to see the comment: \n"
"%(object_url)s\n"
"\n"
"\n"
"(Follow this link to edit your notification preferences: %"
"(notification_settings_url)s )\n"
"\n"
"Your %(site_name)s team.\n"
"\n"
msgstr ""
"Hi, %(receiver_name)s!\n"
"\n"
"%(sender_name)s hat einen Kommentar auf einen Beitrag von dir in der Gruppe %"
"(team_name)s geschrieben:\n"
"\n"
"\"%(comment_text)s\"\n"
"\n"
"\n"
"zu deinem Beitrag:\n"
"\n"
"\"%(note_text)s\"\n"
"\n"
"\n"
"Folge diesem Link um den Kommentar zu sehen: \n"
"%(object_url)s\n"
"\n"
"\n"
"(Folge diesem Link um deine Benachrichtigungseinstellungen zu ändern: %"
"(notification_settings_url)s )\n"
"\n"
"Dein %(site_name)s team.\n"
"\n"

